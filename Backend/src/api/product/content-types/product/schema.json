{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 69,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "dresses",
        "shirts",
        "table",
        "journal"
      ],
      "default": "dresses",
      "required": true
    },
    "size": {
      "type": "string",
      "maxLength": 69
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "Purple",
        "Blue",
        "Black",
        "Red",
        "White",
        "Green"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "availableQty": {
      "type": "integer",
      "required": true
    }
  }
}
